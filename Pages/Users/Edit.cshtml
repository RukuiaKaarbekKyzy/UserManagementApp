@page
@model UserManagementApp.Pages.Users.EditModel

<h2>Edit page</h2>

<form method="post">
    <div>
        <label>Name:</label>
        <input asp-for="User.Name" />
    </div>

    <div>
        <label>Email:</label>
        <input asp-for="User.Email" />
    </div>

    <div>
        <label>Role:</label>
        <input asp-for="User.Role" />
    </div>

    <button type="submit">Save Changes</button>
</form>
